{
  "version": 1,
  "projects": {
    "digital-twin-webapp": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/digital-twin-webapp",
      "sourceRoot": "apps/digital-twin-webapp/src",
      "prefix": "digital-twin-workspace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/digital-twin-webapp",
            "index": "apps/digital-twin-webapp/src/index.html",
            "main": "apps/digital-twin-webapp/src/main.ts",
            "polyfills": "apps/digital-twin-webapp/src/polyfills.ts",
            "tsConfig": "apps/digital-twin-webapp/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/digital-twin-webapp/src/favicon.ico",
              "apps/digital-twin-webapp/src/assets"
            ],
            "styles": ["apps/digital-twin-webapp/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/digital-twin-webapp/src/environments/environment.ts",
                  "with": "apps/digital-twin-webapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "digital-twin-webapp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "digital-twin-webapp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "digital-twin-webapp:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/digital-twin-webapp/tsconfig.app.json",
              "apps/digital-twin-webapp/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/digital-twin-webapp/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/digital-twin-webapp/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "digital-twin-webapp-e2e": {
      "root": "apps/digital-twin-webapp-e2e",
      "sourceRoot": "apps/digital-twin-webapp-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/digital-twin-webapp-e2e/cypress.json",
            "tsConfig": "apps/digital-twin-webapp-e2e/tsconfig.e2e.json",
            "devServerTarget": "digital-twin-webapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "digital-twin-webapp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/digital-twin-webapp-e2e/tsconfig.e2e.json"],
            "exclude": [
              "**/node_modules/**",
              "!apps/digital-twin-webapp-e2e/**/*"
            ]
          }
        }
      }
    },
    "digital-twin-service": {
      "root": "apps/digital-twin-service",
      "sourceRoot": "apps/digital-twin-service/src",
      "projectType": "application",
      "prefix": "digital-twin-service",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/digital-twin-service",
            "main": "apps/digital-twin-service/src/main.ts",
            "tsConfig": "apps/digital-twin-service/tsconfig.app.json",
            "assets": ["apps/digital-twin-service/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/digital-twin-service/src/environments/environment.ts",
                  "with": "apps/digital-twin-service/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "digital-twin-service:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/digital-twin-service/tsconfig.app.json",
              "apps/digital-twin-service/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/digital-twin-service/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/digital-twin-service/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "digital-twin-webapp"
}
