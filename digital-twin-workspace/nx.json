{
  "npmScope": "digital-twin-workspace",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "digital-twin-webapp": {
      "tags": []
    },
    "digital-twin-webapp-e2e": {
      "tags": [],
      "implicitDependencies": ["digital-twin-webapp"]
    },
    "digital-twin-service": {
      "tags": []
    }
  }
}
